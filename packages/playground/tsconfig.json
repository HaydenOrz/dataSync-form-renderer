{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "importHelpers": true,
        "sourceMap": false,
        "baseUrl": "./",
        "paths": {
            "@/*": ["src/*"],
            "@@/*": ["src/.umi/*"]
        },
        "allowSyntheticDefaultImports": true
    },
    "include": [
        "mock/**/*",
        "src/**/*",
        "config/**/*",
        ".umirc.ts",
        "typings.d.ts"
    ],
    "exclude": [
        "node_modules",
        "lib",
        "es",
        "dist",
        "typings",
        "**/__test__",
        "test",
        "docs",
        "tests"
    ]
}
